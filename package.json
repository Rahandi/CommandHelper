{
	"name": "commandhelper",
	"displayName": "Command Helper",
	"description": "Easy helper for using commands",
	"version": "0.3.0",
	"publisher": "Rahandi",
	"repository": "https://github.com/Rahandi/CommandHelper",
	"qna": "marketplace",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:commandhelper.start"
	],
	"icon": "data/code.png",
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "commandhelper.start",
				"title": "Command Helper Start"
			}
		],
		"keybindings": [
			{
				"command": "commandhelper.start",
				"key": "ctrl+shift+q"
			}
		],
		"configuration": {
			"title": "CommandHelper",
			"properties": {
				"command-helper.commands": {
					"type": "object",
					"description": "Set custom commands"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
